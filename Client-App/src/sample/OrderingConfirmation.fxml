<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrderingConfirmation">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="confirmOrderLabel" alignment="TOP_LEFT" layoutX="14.0" layoutY="14.0" prefHeight="369.0" prefWidth="489.0" />
            <Button fx:id="sendOrderButton" layoutX="515.0" layoutY="344.0" mnemonicParsing="false" onAction="#sendOrder" text="send Order" />
            <Button layoutX="533.0" layoutY="296.0" mnemonicParsing="false" text="Print" />
            <Button fx:id="returnButtonCompleteOrder" layoutX="527.0" layoutY="244.0" mnemonicParsing="false" onAction="#closeWindowCompleteOrder" text="Return" />
         </children>
      </AnchorPane>
   </children>
</VBox>
